               , data-default-class < 0.1
               , operational >= 0.2.2
               , Chart >= 0.17

  Exposed-modules:
        Graphics.Rendering.Chart.Backend.Cairo

source-repository head
  type:     git
  location: https://github.com/timbod7/haskell-chart
